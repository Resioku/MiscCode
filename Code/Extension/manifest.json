{
    "manifest_version": 3,
    "name": "Test Extension",
    "version": "0.3",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup/index.html",
        "default_title": "A pop-up will appear here."
    }
}